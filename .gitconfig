[gpg]
	format = ssh
[gpg "ssh"]
	program = /Applications/1Password.app/Contents/MacOS/op-ssh-sign
[commit]
	gpgsign = true
[user]
	signingkey = signingkey
	name = Your Name
	email = your@email.com
[push]
	autoSetupRemote = true
[core]
	excludesfile = /Users/USER_NAME/.gitignore
[pull]
	ff = only
[lfs "customtransfer.xet"]
	path = git-xet
	args = transfer
	concurrent = true
